fun sum(a: Int, b: Int): Int {
    return a + b  // Return the sum of the two numbers
}

fun main() {
    // Call the sum function with different values
    val result1 = sum(5, 3)
    val result2 = sum(10, 20)

    // Display the results
    println("Sum of 5 and 3: $result1")
    println("Sum of 10 and 20: $result2")
}
