// Define the Car class
class Car(val brand: String, val model: String, val year: Int) {
    // Function to display car details
    fun displayDetails() {
        println("Car brand: $brand, Model: $model, Year: $year")
    }
}

fun main() {
    // Create two objects of the Car class
    val car1 = Car("Toyota", "Corolla", 2020)
    val car2 = Car("Honda", "Civic", 2021)

    // Call the displayDetails function for each object
    car1.displayDetails()
    car2.displayDetails()
}
